<!DOCTYPE TMD>
<TMD>
	<name>servicioscli</name>
	<!--Servicios a clientes-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Servicios a Clientes")</alias>

	<field>
		<name>idservicio</name>
		<!--Identificador interno de la tabla-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Identificador")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>serial</type>
		<visiblegrid>false</visiblegrid>
		<relation>
			<table>lineasservicioscli</table>
			<field>idservicio</field>
			<card>1M</card>
		</relation>
		
	</field>

	<field>
		<name>numservicio</name>
		<!--Código identificativo del servicio-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Número")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>
		<counter>true</counter>
		<unique>true</unique>
	</field>


	<field>
		<name>fecha</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Fecha")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>date</type>
	</field>
	
	<field>
        <name>nombrecliente</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Cliente")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>string</type>
        <length>100</length>

    </field>
		
	<field>
		<name>codcliente</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Cod. cliente")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>6</length>

		<relation>
			<table>clientes</table>
			<field>codcliente</field>
			<card>M1</card>
		</relation>
	</field>

	<field>
		<name>matricula</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Matricula")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>10</length>
	</field>
	

	<field>
		<name>editable</name>
		<!--Indica si el servicio puede ser modificado-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Editable")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>unlock</type>
		<default>true</default>
		<visiblegrid>true</visiblegrid>
	</field>
	

	<field>
		<name>codtecnico</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Mecánico")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>5</length>
		<null>false</null>
		<relation>
			<table>tecnicos</table>
			<field>codtecnico</field>
			<card>M1</card>
		</relation>
	</field>
	
	

	<field>
		<name>contratomant</name>
		<!--Indica si el servicio entra en un contrato de mantenimiento-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Contrato de mantenimiento")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
		<default>false</default>
	</field>
	
	

	<field>
		<name>horas</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Tiempo empleado (H/M)")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>3</partI>
		<partD>0</partD>
		<default>0</default>
		<visiblegrid>false</visiblegrid>
	</field>

	<field>
		<name>minutos</name>
