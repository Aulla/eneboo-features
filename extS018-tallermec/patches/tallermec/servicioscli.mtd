		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>3</partI>
		<partD>2</partD>
	</field>
	<field>
		<name>codvehiculo</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Vehículo")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>8</length>	
		<relation>
			<table>vld_vehiculos</table>
			<field>codvehiculo</field>
			<card>M1</card>
			<delC>true</delC>
		</relation>
		<associated>
			<with>codcliente</with>
			<by>codcliente</by>
		</associated>
	</field>

